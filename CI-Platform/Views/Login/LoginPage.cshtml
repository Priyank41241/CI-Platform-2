                <div>
                    <div class="loginlabel">
                        <label for="emailin">Email Address</label>
                    </div>
                    <input class="form-control " id="emailin"
                           placeholder="Email"
                           type="text">
                </div>
                <div>
                    <div class="loginlabel">
                        <label for="pass">Password</label>
                    </div>
                    <input id="pass"
                           class="form-control" placeholder="Password"
                           type="password">
                </div>
                <div>
                    <input type="submit" class=" form-control loginbtn"
                           value="Login">
                </div>
                <div class="forgetpasspage">
                    <a asp-controller="Login" asp-action="ForgotPass" class="forgetpasslink"
                       id="forgetpass">
                        Lost
                        your password?
                    </a>
                </div>
                <div class="register">
                    Don't have an account?<a asp-controller="Login" asp-action="Registration" id="registerlink">
                        Create an account
                    </a>
                </div>
            </form>
            <div class="text-center">
                <a href="#" id="privacytxt"> Privacy Policy</a>
            </div>
